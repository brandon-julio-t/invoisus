{
  "name": "invoisus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "d": "npm-run-all --parallel dev:frontend dev:backend",
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "next dev --turbopack",
    "dev:backend": "convex dev",
    "predev": "convex dev --until-success && convex dev --once --run-sh \"node setup.mjs --once\" && convex dashboard",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "format": "prettier --write . --cache --cache-location .prettiercache",
    "typecheck": "next typegen && tsc --noEmit --tsBuildInfoFile .tsbuildinfo",
    "check": "npm-run-all format lint typecheck",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --cache-location .eslintcache",
      "prettier --write --cache --cache-location .prettiercache"
    ],
    "*.{json,css,md,html}": [
      "prettier --write --cache --cache-location .prettiercache"
    ]
  },
